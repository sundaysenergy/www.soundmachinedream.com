&lt;div class=&quot;field field-type-filefield field-field-image&quot;&gt;
    &lt;div class=&quot;field-items&quot;&gt;
            &lt;div class=&quot;field-item odd&quot;&gt;
                    &lt;div class=&quot;filefield-file&quot;&gt;&lt;img class=&quot;filefield-icon field-icon-image-gif&quot;  alt=&quot;image/gif icon&quot; src=&quot;http://www.soundmachinedream.com/sites/all/modules/contrib/filefield/icons/image-x-generic.png&quot; /&gt;&lt;a href=&quot;//files/soundmachinedream/album_art/tlc_front.gif&quot; type=&quot;image/gif; length=28367&quot;&gt;tlc_front.gif&lt;/a&gt;&lt;/div&gt;        &lt;/div&gt;
        &lt;/div&gt;
